public class TeaMaker {

    
    private Storage teaBag;
    private boolean withSugar;
    private String topping;
    private static int amountOfDrinks; 
    
    public TeaMaker(String teaType, boolean withSugar, String topping){
        this.teaType = teaType;
        this.withSugar = withSugar;
        this.topping = topping;
        amountOfDrinks = 0;
    }
}
